#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        vector<long long> v(n);
        int ts=0;
        int oc=0;
        for(int i=0;i<n;i++){
            cin >> v[i];
            ts=ts+v[i];
            if(v[i]%2!=0){
                oc++;
            }
        }
        if(ts%2==0){
            cout << "NO" << endl;
        }
        else{
            if(oc==1){
                cout << "YES" << endl;
            }
            else{
                cout << "NO" << endl;
            }
        }
    }
}

