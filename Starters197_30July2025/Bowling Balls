#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--){
        long long n,x,y;
        cin >> n >> x >> y;
        vector<long long> v(n);
        int ans=0;
        for(int i=0;i<n;i++){
            cin >> v[i];
            if(v[i]>=x && v[i]<=y){
                ans++;
            }
        }
        cout << ans << endl;
        
    }
}
