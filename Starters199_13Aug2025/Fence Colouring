#include <bits/stdc++.h>
using namespace std;

int main() {
 int t;
 cin >> t;
 while(t--){
     long long n;
     cin >> n;
     vector<long long> v(n);
     for(int i=0;i<n;i++){
         cin >> v[i];
     }
     int p=v[0];
     int ans=0;
     for(int i=1;i<n;i++){
         if(p>=v[i]){
             continue;
         }
         else{
             ans=i;
             p=v[i];
         }
     }
     cout << ans+1 << endl;
 }
}
