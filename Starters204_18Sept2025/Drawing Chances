#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int m,n;
	    cin >> n>>m;
	    string s;
	    cin >> s;
	    int al=0,bo=0;
	    for(int i=0;i<m;i++){
	        if(s[i]=='1'){
	            al++;
	        }
	        else{
	            bo++;
	        }
	    }
        
        
        
        if(n%2==0){
            int r= n-m;
            int alr=n/2-al;
            int blr=n/2-bo;
            if(alr>=0 && blr>=0 && alr+blr==r){
                cout << "Yes" << endl;
            }
            else{
                cout << "No" << endl;
            }
        }
         else{
                cout << "No" << endl;
            }
	}

}
