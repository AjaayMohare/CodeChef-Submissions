#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        if(n%4==0){
            cout << (n/4)*400 << endl;
        }
        else if(n%4==3){
            cout << (n/4)*400 + 300 << endl;
        }
        else if(n%4==2){
            cout << (n/4)*400 + 200 << endl;
        }
        else{
            if(n >= 4){
                cout << (n/4 - 1)*400 + 300 + 200 << endl;
            }
            else{
                cout << 200 << endl;
            }
        }
    }
}

