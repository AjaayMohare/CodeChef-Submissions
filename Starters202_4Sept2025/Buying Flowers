#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        if (n % 3 == 0) {
            printf("%d\n", 5 * (n / 3));
        }
        else if (n % 3 == 1) {
            printf("%d\n", (n / 3 - 1) * 5 + 8);
        }
        else {
            printf("%d\n", (n / 3) * 5 + 4);
        }
    }
}


