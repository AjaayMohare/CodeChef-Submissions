#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--){
        long long a,b,c,d;
        cin >> a >> b >> c >>d;
    int m= 5*(a)+b;
    int n= 5*(c)+d;
    if(m>=n && ((m-n)%6==0)){
        cout << "yes" << endl;
    }
   
    else{
        cout << "no" << endl;
    }
        
    }
}


