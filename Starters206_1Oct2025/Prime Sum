#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
     int t;
     cin >> t;
     while(t--){
         int n;
         cin >> n;

         vector<int> v(n);
         for(int i=0;i<n;i++){
             cin >> v[i];
         }
         
         int ans=0;
         for(int i=0;i<n;i++){
             for(int j=i+1;j<n;j++){
                 if((v[i]+v[j])==2 ||(v[i]+v[j])==3||(v[i]+v[j])==5 ){
                     ans++;
                 }
             }
         }
         cout << ans << endl;
     }
}
